@model IEnumerable<StudyModel>

@if (Model != null && Model.Count() > 0)
{
    <table class="table">
        <thead>
            <tr>
                <th>Lists</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        <a asp-action="Details" asp-controller="Study" asp-route-listName="@item.ListName" class="text-uppercase">@item.ListName</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
}
